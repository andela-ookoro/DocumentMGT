  "scripts": {
    "start": "node lib/serverProduction.js",
    "local:build": "webpack --config webpack.config.js",
    "dev": "nodemon --watch server --exec babel-node  -- server.js",
    "start:dev": "nodemon ./bin/www",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "clean": "rimraf lib && mkdir lib && mkdir lib/public && mkdir lib/public/apidoc/",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress --colors && npm run copy:apiDocs",
    "copy:apiDocs": "cp -a public/apidoc/. lib/public/apidoc/ && npm run copy:publicfiles",
    "copy:publicfiles": "cp -a public/{index.html,logo.png,favicon.ico} lib/public/ && cp -a public/lib lib/public",
    "build:server": "babel server -d lib/server --copy-files --ignore tests/ && npm run build-app",
    "build-server-test": "babel server/tests -d lib/server/tests --copy-files",
    "build-app": "babel serverProduction.js -d lib",
    "client-test": "jest  --config jest.config.js -u  && npm run server-coverage",
    "client-coverage": "codecov --token=1ae99ab6-3079-408d-98c6-0176d944efbb",
    "e2e-test": "nightwatch",
    "acceptanceTest": "run-p -r start  test-e2e",
    "server-pretest": "DATABASE_URL=postgres://sdneoiwa:CGgqmi-vCzC2IFd3dJWpcxwhj3mrAjMe@pellefant.db.elephantsql.com:5432/sdneoiwa NODE_ENV=test sequelize db:migrate ",
